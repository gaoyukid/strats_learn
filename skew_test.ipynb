    "import pandas as pd\n",
    "import yfinance as yf\n",
    "\n",
    "# 下载特斯拉历史数据\n",
    "tsla = yf.download(\"TSLA\", start=\"2020-01-01\", end=\"2023-10-01\")\n",
    "returns = tsla['Close'].pct_change().dropna()\n",
    "\n",
    "# 计算偏度\n",
    "skewness = returns.skew()\n"
   ]
